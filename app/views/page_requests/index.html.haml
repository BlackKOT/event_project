- title "Page Requests"
%table
  %tr
    %th Path
    %th Page Duration
    %th View Duration
    %th Db Duration
  - for page_request in @page_requests
    %tr
      %td= page_request.path
      %td= page_request.page_duration
      %td= page_request.view_duration
      %td= page_request.db_duration
